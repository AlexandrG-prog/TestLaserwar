<Window x:Class="TestLaserwar.Windows.Subdivision.SubdivisionEmptyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestLaserwar.Windows.Subdivision"
        mc:Ignorable="d"
        Title="" Height="450" Width="800">
    <Grid>
        <Button Style="{StaticResource ButtonTemplate}" Content="&lt;-" Command="{Binding Back}" HorizontalAlignment="Left" Margin="28,24,0,0" VerticalAlignment="Top" Width="49" Height="36" FontWeight="Bold" FontSize="16" FontFamily="Showcard Gothic"/>
        <Label Content="{Binding SubdivisionName}" HorizontalAlignment="Left" Margin="121,24,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.141,0.087" Height="36" Width="65" FontWeight="Bold" FontSize="18"/>
        <Button Style="{StaticResource ButtonTemplate}" Command="{Binding AddSubdivision}" Content="Add subdivision" Margin="28,0,0,26" Width="110" Height="29" VerticalAlignment="Bottom" HorizontalAlignment="Left"/>
        <Button Style="{StaticResource ButtonTemplate}" Command="{Binding AddEmployee}" Content="Add employee" HorizontalAlignment="Left" Margin="234,0,0,26" VerticalAlignment="Bottom" Width="100" Height="29"/>
    </Grid>
</Window>
